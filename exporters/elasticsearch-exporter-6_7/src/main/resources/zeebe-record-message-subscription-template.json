{
  "index_patterns": [
    "zeebe-record_message-subscription_*"
  ],
  "version": 1,
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0,
    "index.queries.cache.enabled": false
  },
  "aliases": {
    "zeebe-record-message-subscription": {}
  },
  "mappings": {
    "_doc": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "processInstancePartitionId": {
              "type": "integer"
            },
            "processInstanceKey": {
              "type": "long"
            },
            "elementInstanceKey": {
              "type": "long"
            },
            "messageName": {
              "type": "keyword"
            },
            "correlationKey": {
              "type": "text"
            },
            "bpmnProcessId": {
              "type": "keyword"
            },
            "messageKey": {
              "type": "long"
            },
            "variables": {
              "enabled": false
            },
            "interrupting": {
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}
