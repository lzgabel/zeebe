{
  "index_patterns": [
    "zeebe-record_message_*"
  ],
  "version": 1,
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0,
    "index.queries.cache.enabled": false,
    "index": {
      "lifecycle": {
        "name": "zeebe-policy"
      }
    }
  },
  "aliases": {
    "zeebe-record-message": {}
  },
  "mappings": {
    "_doc": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "name": {
              "type": "keyword"
            },
            "correlationKey": {
              "type": "text"
            },
            "messageId": {
              "type": "keyword"
            },
            "timeToLive": {
              "type": "long"
            },
            "variables": {
              "enabled": false
            },
            "deadline": {
              "type": "long"
            }
          }
        }
      }
    }
  }
}
